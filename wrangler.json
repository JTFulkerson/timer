{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "timer-app",
  "compatibility_date": "2025-09-02",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "main": "@tanstack/react-start/server-entry",
  "assets": {
    "directory": "public",
    "binding": "ASSETS"
  },
  "vars": {
    "NODE_ENV": "production"
  },
  "observability": {
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "invocation_logs": true,
      "persist": true
    }
  },
  "routes": [
    {
      "pattern": "timer.johnfulkerson.com",
      "custom_domain": true
    }
  ]
}